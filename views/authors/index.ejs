<h2>Search Authors</h2>
<form action="/authors" method="GET">
  <label for="search-input">Name</label>
  <input
    type="text"
    name="name"
    id="search-input"
    value="<%= searchOptions.name %>"
  />
  <button type="submit">Search</button>
</form>
<br />
<% authors.forEach(author => { %>
<div><%= author.name %></div>
<% }) %>
